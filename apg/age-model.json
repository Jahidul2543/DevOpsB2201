{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Person",
  "description": "A person",
  "type": "object",
  "properties":
  {
    "name":
    {
      "description": "A person's name",
      "type": "string"
    },
    "age":
    {
      "description": "A person's age",
      "type": "number",
      "minimum": 10,
      "maximum": 65
    }
  },
  "required": ["name", "age"]
}